<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="shardulgogate.matchday">

	<application
		android:allowBackup="true"
		android:icon="@mipmap/ic_launcher"
		android:label="@string/app_name"
		android:roundIcon="@mipmap/ic_launcher_round"
		android:supportsRtl="true"
		android:theme="@style/AppTheme">
		<activity android:name=".HighlightsActivity"></activity>
		<activity android:name=".EventActivity" />
		<activity android:name=".MatchActivity" />
		<activity android:name=".ViewSquad" />
		<activity android:name=".AddPlayer" />
		<activity android:name=".SquadInfo" />
		<activity android:name=".Duration" />
		<activity android:name=".HomeScreen" />
		<activity android:name=".MainActivity">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />

				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>

		<meta-data
			android:name="preloaded_fonts"
			android:resource="@array/preloaded_fonts" />
	</application>

</manifest>